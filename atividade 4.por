programa {
  funcao inicio() {
    // #4- Faça um algoritmo que receba o preço de custo de um produto e informe o preço para venda, 
    //sabendo que, o preço de custo será acrescido de acordo com um percentual informado pelo usuário.


  real a, b
  

      escreva("Preço do produto: "," R$")
      leia(a)
      escreva("Imposto sobre o produdo: ","%")
      leia(b)
          escreva("Preço de venda: "," R$", a+(a*(b/100)))

    
  }
}
